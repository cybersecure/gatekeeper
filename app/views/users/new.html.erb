<h1>Some App</h1>
<div class="block" id="block-signup">
  <h2>Sign up</h2>
  <p>Already have an account? <%= link_to "Log in", login_path %>.</p>
  <div class="content">
    <div class="flash">
      <% flash.each do |type, message| %>
      <div class="message <%= type %>">
        <p><%=  message %></p>
      </div>
      <% end %>
    </div>
    <%= render 'form' %>
  </div>
</div>
